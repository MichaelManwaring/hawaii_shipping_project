<%= form_for @user, url: user_path(@user), method: :put do |n| %>
  <%= n.label(:email, "Email:") %>
  <%= n.email_field(:email) %>
  <%= n.label(:password, "Password:") %>
  <%= n.password_field(:password) %>
  <%= n.label(:password_confirmation, "Confirm Password:") %>
  <%= n.password_field(:password_confirmation) %>
  <%= n.label(:company, "Company Name:") %>
  <%= n.text_field(:company) %>
  <%= n.label(:fname, "First Name:") %>
  <%= n.text_field(:fname) %>
  <%= n.label(:lname, "last Name:") %>
  <%= n.text_field(:lname) %>
  <%= n.submit("Edit Profile") %>
<% end %>